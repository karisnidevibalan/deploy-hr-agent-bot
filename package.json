{
  "name": "hr-agent-bot",
  "version": "1.0.0",
  "description": "AI-powered HR Agent for Winfomi",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/src/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc && node scripts/copyData.js",
    "prod": "npm run build && node dist/src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test",
    "demo": "node demo-server.js",
    "vercel-build": "npm install && npm run build",
    "now-build": "npm run build",
    "docker:build": "docker build -t hr-agent-bot .",
    "docker:run": "docker run -p 5000:5000 --env-file .env.local hr-agent-bot"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.9",
    "axios": "^1.5.0",
    "chrono-node": "^2.9.0",
    "compromise": "^14.3.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.3.3",
    "jsforce": "^3.10.10",
    "mammoth": "^1.11.0",
    "multer": "^2.0.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^8.0.1",
    "pdf-parse": "^2.4.5",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.19.27",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@types/xlsx": "^0.0.35",
    "@vercel/node": "^5.5.16",
    "baseline-browser-mapping": "^2.9.9",
    "fs-extra": "^11.3.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
